<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <OutputType>WinExe</OutputType>
    <ProductName>MEGAbolt</ProductName>
    <ApplicationIcon>MB.ico</ApplicationIcon>
    <PublisherName>Sjofn LLC</PublisherName>
    <DisallowUrlActivation>true</DisallowUrlActivation>
    <UseApplicationTrust>true</UseApplicationTrust>
	<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <UseWindowsForms>true</UseWindowsForms>
	<PublishReadyToRun>true</PublishReadyToRun>
	<PublishSingleFile>false</PublishSingleFile>
	<SelfContained>true</SelfContained>
	<BootstrapperEnabled>true</BootstrapperEnabled>
	<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
	<OutputPath>..\bin\$(Configuration)\</OutputPath>
	<Platforms>AnyCPU;x64;x86</Platforms>
	<StartupObject>MEGAbolt.Program</StartupObject>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
	<Optimize>true</Optimize>
	<DebugType>pdbonly</DebugType>
  </PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
	<DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
	<Optimize>true</Optimize>
	<DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
	<DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x86'">
	<RuntimeIdentifier>win-x86</RuntimeIdentifier>
	<Optimize>true</Optimize>
	<DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
	<RuntimeIdentifier>win-x86</RuntimeIdentifier>
	<DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
	<BugsplatDatabase Condition="'$(BugsplatDatabase)' == ''">MEGAbolt</BugsplatDatabase>
  </PropertyGroup>
